{
  "name": "sample-cli",
  "version": "0.1.0",
  "description": "提升效率的脚手架",
  "main": "dist/bundle.min.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/bundle.min.js"
    }
  },
  "bin": {
    "sample-cli": "./dist/bundle.min.js",
    "sc": "./dist/bundle.min.js"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin rollup-plugin-typescript2",
    "dev": "tsx watch ./src/index.ts",
    "ui:dev": "pnpm -C packages/sample-ui dev",
    "doc": "pnpm -C docs docs:dev",
    "spider": "tsx watch ./src/spider.ts",
    "postinstall": "tsx scripts/setup.ts"
  },
  "author": "yi.yang",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.11",
    "@types/node-emoji": "^1.8.2",
    "rollup": "^3.19.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1"
  },
  "dependencies": {
    "cfonts": "^3.1.1",
    "chalk": "^5.2.0",
    "inversify": "^6.0.1",
    "node-emoji": "^1.11.0",
    "open": "^9.1.0",
    "ora": "^6.3.0",
    "os-name": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "commander": "^10.0.0",
    "cross-spawn": "^7.0.3",
    "download": "^8.0.0",
    "download-git-repo": "^3.0.2",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^9.1.5",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "semver": "^7.5.1",
    "tslib": "^2.5.0",
    "tsx": "^3.12.6",
    "typescript": "^4.9.5"
  },
  "files": ["scripts", "dist", "template"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yyi0708/sample-cli.git"
  },
  "keywords": [
    "cli",
    "sample-cli"
  ]
}